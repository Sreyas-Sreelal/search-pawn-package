{
    "name": "search-pawn-package",
    "displayName": "search-pawn-package",
    "description": "vscode extension providing suggestion for pawn dependencies by using pawndex api",
    "version": "0.0.3",
    "publisher": "sreyassreelal",
    "engines": {
        "vscode": "^1.26.0"
    },
    "bugs": {
        "url": "https://github.com/Sreyas-Sreelal/search-pawn-package/issues",
        "email": "sreyassreelal@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Sreyas-Sreelal/search-pawn-package.git"
    },
    "categories": [
        "Other",
        "Programming Languages"
    ],
    "activationEvents": [
        "workspaceContains:pawn.json"
    ],
    "main": "./out/extension",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "axios": "^0.18.0"
    }
}
